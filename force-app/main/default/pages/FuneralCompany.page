        function setSaijoShiborikomiJoken(objId, hdnId){
            var hdnVal = document.getElementById('idPage:myFormId:' + hdnId).value;
            if(hdnVal != null && hdnVal != ''){
                var splHdnVal = hdnVal.split(';');
                var options = document.getElementById('idPage:myFormId:' + objId).options;
                for(var i = 0; i < options.length; i++){
                    for(var j = 0; j < splHdnVal.length; j++){
                        if(splHdnVal[j] == options[i].value){
                            options[i].selected = true;
                            break;
                        }
                    }
                }
            }
        }

        function multiselectValue(objId, hdnId){
            var options = document.getElementById('idPage:myFormId:' + objId).options;
            var sltVal = '';
            for(var i = 0; i < options.length; i++){
                if(options[i].selected){
                    sltVal += options[i].value + ';';
                }
            }
            document.getElementById('idPage:myFormId:' + hdnId).value = sltVal;
        }

