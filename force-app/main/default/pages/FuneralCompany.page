        function attrLatLngFromAddress(address, hallJokenClear){
            var geocoder = new google.maps.Geocoder();
            geocoder.geocode({'address': address}, function(results, status){
                var administrative_area_level_1 = '';
                var administrative_area_level_2 = '';
                var shikugun = '';
                var sublocality = '';
                if(status == google.maps.GeocoderStatus.OK) {
                    var address_components = results[0].address_components;
                    for(var i=0; i<address_components.length; i++){
                        var types = address_components[i].types;
                        if(types[0] == 'administrative_area_level_1'){
                            administrative_area_level_1 = address_components[i].long_name;
                        }
                        if(types[0] == 'administrative_area_level_2'){
                            administrative_area_level_2 = address_components[i].long_name;
                        }
                        else if(types[0] == 'locality'){
                            shikugun = address_components[i].long_name;
                        }
                        else{
                            for(var j=0; j<types.length; j++){
                                if(types[j] == 'sublocality_level_1'){
                                    sublocality = address_components[i].long_name;
                                    break;
                                }
                            }
                        }
                    }

                    var formatted_address = results[0].formatted_address;
                    var lat = results[0].geometry.location.lat();
                    var lng = results[0].geometry.location.lng();

                    // 小数点第六位以下を四捨五入した値を緯度経度にセット、小数点以下の値が第六位に満たない場合は0埋め
                    var latitudeJs = (Math.round(lat * 1000000) / 1000000).toFixed(6);
                    var longitudeJs = (Math.round(lng * 1000000) / 1000000).toFixed(6);
                    
                    if(document.getElementById('idPage:myFormId:searchTypeHiddenId').value == 'idKiboShikugunArea'){
                        $j('[id$="shikugunNameHiddenId"]').val(document.getElementById('idPage:myFormId:idKiboShikugunArea').value);
                    }
                    else{
                        $j('[id$="shikugunNameHiddenId"]').val(administrative_area_level_1 + administrative_area_level_2 + shikugun + sublocality);
                    }
                    $j('[id$="kensakuchitenjusho"]').val(formatted_address);
                    $j('[id$="latitudeHiddenId"]').val(latitudeJs); 
                    $j('[id$="longitudeHiddenId"]').val(longitudeJs);
                    actionSearch(hallJokenClear);
                }
                else{
                    alert('検索結果がありません');
                    hideLoadingDialog();
                }
            });
        }

